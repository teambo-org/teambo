<div class="member-index">
    <nav class="breadcrumbs">
        <a href="#{{team.url}}" class="active" data-keybind="q escape `">Team</a>
        <span class="active">Members</span>
    </nav>
    <nav>
    {{#team.isAdmin}}
        <a href="#{{team.url}}/members/new" data-keybind="e">New Invite</a>
    {{/team.isAdmin}}
    </nav>
    <h1><i class="icon-flag-filled"></i> Members</h1>
    <ul class="buckets member" id="members">
    {{#model.member.all}}
        <li class="member {{#opts.pubKey}}hi-10{{/opts.pubKey}}{{^opts.pubKey}}color-bg-05{{/opts.pubKey}} up1c" id="member-li-{{id}}">
            <a href="#{{url}}" class="hi-05">
                {{^opts.pubKey}}
                <span class="pending">Pending</span>
                {{/opts.pubKey}}
                <div class="img"><i class="{{icon}}"></i></div>
                <span class="name">{{opts.name}}</span>
            </a>
        </li>
    {{/model.member.all}}
    {{^model.member.all}}
    <p class="empty">Team has no members</p>
    {{/model.member.all}}
    </ul>
</div>
