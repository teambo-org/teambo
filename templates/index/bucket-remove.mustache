<div class="bucket-remove" data-tpljs="index/bucket-remove">
    <nav class="breadcrumbs">
        <a href="#{{team.url}}">Team</a>
        <a href="#{{bucket.url}}" data-keybind="q escape">Bucket</a>
        <span>Remove</span>
    </nav>
    <h1>Remove Bucket</h1>
    <form id="bucket-remove" name="bucket_remove" method="POST" class="std ext">
        <input type="hidden" class="hidden" name="id" value="{{bucket.id}}"/>
        <p>Are you sure you want to remove this bucket?</p>
        <div class="field submit">
            <input type="submit" class="submit" value="Remove Bucket" id="delete_submit" />
            <br/>
            <a class="cancel" href="#{{bucket.url}}">cancel</a>
        </div>
        <div class="error"></div>
    </form>
</div>
