<div class="bucket-remove" data-tpljs="index/bucket-remove">
    <h1>Delete Bucket</h1>
    <form id="bucket-remove" name="bucket_remove" method="POST" class="std ext">
        <input type="hidden" class="hidden" name="id" value="{{bucket.id}}"/>
        <p>Are you sure you want to delete this bucket?</p>
        <div class="field submit">
            <input type="submit" class="submit" value="Delete Bucket" />
            <br/>
            <a class="cancel" href="#/{{team.id}}/{{bucket.id}}">cancel</a>
        </div>
        <div class="error"></div>
    </form>
</div>
