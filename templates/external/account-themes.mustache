{{#teams}}
#team-li-{{id}} .i-wrap { background-color: rgba({{#theme}}{{main.bg}}{{/theme}}, 1); }
#team-li-{{id}} i { background-color: rgba({{#theme}}{{main.hi}}{{/theme}}, 0.4); color: rgba({{#theme}}{{main.color}}{{/theme}}, 0.9); }
#team-li-{{id}} .name { background-color: rgba({{#theme}}{{main.bg}}{{/theme}}, 1); }
#team-li-{{id}} .name > div { background-color: rgba({{#theme}}{{main.hi}}{{/theme}}, 0.2); }
#team-li-{{id}} a { color: rgba({{#theme}}{{right.color}}{{/theme}}, 0.9); }
{{/teams}}
