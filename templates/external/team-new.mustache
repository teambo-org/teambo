<div class="external require-auth c std" data-tpljs="external/team-new">
    <div id="logo" class="logo"></div>
    <form id="team-new" name="team_new" method="POST" class="std ext">
        <div class="field">
            <input type="text" class="text required min-len-2" name="name" placeholder="Team Name" />
        </div>
        <div class="field submit">
            <input type="submit" class="submit" value="Create Team" />
            <br/>
            <a class="cancel" href="#/account">cancel</a>
        </div>
        <div class="error"></div>
    </form>
    <div class="subtext">
        <p>Accounts are currently limited to 3 teams</p>
    </div>
</div>
