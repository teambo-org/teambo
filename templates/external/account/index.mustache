<div class="external account require-auth c">
    <div id="logo" class="logo"></div>
    <div class="nav">
        <a class="logout" href="#/logout"><i class="icon-power" title="Log Out"></i></a>
        <p>Logged in as {{acct.current.email}}</p>
    </div>
    <div class="main">
        <div id="status"></div>
        <ul id="teams" class="teams">
        {{#acct.current.teams}}
        <li class="team placeholder">&nbsp;</li>
        {{/acct.current.teams}}
        {{^acct.current.teams}}
            <li class="welcome" id="welcome">
                <h3>Welcome to your new account</h3>
                <p>You can create a new team by clicking the link below or you can contact a member
                of an existing team and ask them to invite you</p>
            </li>
        {{/acct.current.teams}}
        </ul>
        <a href="#/team/new" class="team-new" id="team-new">Create Team</a>
    </div>
</div>
<link id="account-themes" type='text/css' rel='stylesheet'/>
