<div class="external account require-auth c" data-tpljs="external/account">
    <div id="logo" class="logo"></div>
    <div class="nav">
        <a class="logout" href="#/logout"><i class="icon-power" title="Log Out"></i></a>
        <p>Logged in as {{acct.email}}</p>
    </div>
    <div class="main">
        <ul id="teams" class="teams">
        {{^acct.teams}}
            <li class="placeholder">
                <h3>Welcome to your new account</h3>
                You can create a new team by clicking the link below or you can contact a member 
                of an existing team and ask them to invite you
            </li>
        {{/acct.teams}}
        </ul>
        <a href="#/team-new" class="team-new replace" data-keybind="c">Create Team</a>
    </div>
</div>
<link id="account-themes" type='text/css' rel='stylesheet'/>
